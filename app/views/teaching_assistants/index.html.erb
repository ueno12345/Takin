<p style="color: green"><%= notice %></p>

<h1>TA一覧画面</h1>

<%= form_tag import_teaching_assistants_path, multipart: true do %>
  <%= file_field_tag :file %>
  <%= submit_tag "インポート" %>
<% end %>

<div id="teaching_assistants">
  <% @teaching_assistants.each do |teaching_assistant| %>
    <%= render teaching_assistant %>
    <p>
      <%= link_to "Show this teaching assistant", teaching_assistant %>
    </p>
  <% end %>
</div>

<%= link_to "New teaching assistant", new_teaching_assistant_path %>
